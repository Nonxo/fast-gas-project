{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.0",
    "@types/expo": "^32.0.13",
    "@types/react-native-tab-view": "^1.0.6",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-swiper": "^1.5.14",
    "react-native-tab-view": "^2.2.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-navigation": "^3.3.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.57.40",
    "@types/react-native-swiper": "^1.5.10",
    "@types/react-navigation": "^3.0.5",
    "@types/react-redux": "^7.0.3",
    "@types/react-test-renderer": "^16.8.1",
